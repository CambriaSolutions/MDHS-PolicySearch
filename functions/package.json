    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "firebase-admin": "~6.1.0",
    "firebase-functions": "^2.1.0",
    "firebase-tools": "^6.1.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "mkdirp-promise": "^5.0.1",
    "pdf2json": "^1.1.7",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-plugin-promise": "^4.0.1"
